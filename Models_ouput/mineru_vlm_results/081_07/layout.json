                                {
                                    "bbox": [
                                        173,
                                        2314,
                                        1188,
                                        2859
                                    ],
                                    "type": "text",
                                    "content": " (see equation 13) and the damping length "
                                },
                                {
                                    "bbox": [
                                        173,
                                        2314,
                                        1188,
                                        2859
                                    ],
                                    "type": "inline_equation",
                                    "content": "l_d\\propto \\omega^{- 1}"
                                },
                                {
                                    "bbox": [
                                        173,
                                        2314,
                                        1188,
                                        2859
                                    ],
                                    "type": "text",
                                    "content": " for a fixed "
