                                    "bbox": [
                                        146,
                                        235,
                                        1218,
                                        1420
                                    ],
                                    "type": "inline_equation",
                                    "content": "V_{\\mathrm{isr}}(\\mathrm{C^{18}O}) - V_{\\mathrm{isr}}(\\mathrm{DCO^{+}})"
                                },
