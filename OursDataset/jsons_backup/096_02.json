    },
    {
      "category_type": "plain_text",
      "poly": [
        1282,
        956,
        2272,
        956,
        2272,
        1082,
        1282,
        1082
      ],
      "ignore": false,
      "order": "10",
      "anno_id": 1,
      "text": " principal point  $(c_u, c_v)$ . The subscripts *u* and *v* indicate the horizontal and vertical directions, respectively. The mapping is formulated as",
      "line_with_spans": [
        {
          "category_type": "text_span",
          "poly": [
            1282,
            956,
            2272,
            956,
            2272,
