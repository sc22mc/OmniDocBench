    {
      "category_type": "plain_text",
      "poly": [
        1286,
        1308,
        2272,
        1308,
        2272,
        1398,
        1286,
        1398
      ],
      "ignore": false,
      "order": 13,
      "anno_id": 1,
      "text": " Kannala and Brandt [\\[16\\]](#page-16-1) proposed the generic camera model, which includes fisheye lens cameras and is given by",
      "line_with_spans": [
        {
          "category_type": "text_span",
          "poly": [
            1286,
            1308,
            2272,
            1308,
            2272,
