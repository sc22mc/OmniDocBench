            "poly": [
                219.9999999999999,
                582.5,
                2257.5,
                582.5,
                2257.5,
                725,
                219.9999999999999,
                725
            ],
            "ignore": false,
            "order": 4,
            "anno_id": 1,
            "text": " We evaluate our model on three different datasets, AD, 2AA, and 4AA, as introduced in Section 6. All datasets are simulated in implicit solvent using the openMM library (Eastman et al., 2017). For all MD simulations we use the parameters shown in Table 2.",
            "line_with_spans": [
                {
                    "category_type": "text_span",
                    "poly": [
                        219.9999999999999,
                        582.5,
                        2257.5,
                        582.5,
                        2257.5,
                        725,
                        219.9999999999999,
                        725
                    ],
                    "text": ""
                }
            ],
            "attribute": {
